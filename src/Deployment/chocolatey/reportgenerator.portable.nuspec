<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>reportgenerator.portable</id>
    <version>4.0.0.0</version>
    <title>ReportGenerator (Portable) - Generates reports for various code coverage tools</title>
    <authors>Daniel Palme</authors>
    <owners>Daniel Palme</owners> 
    <licenseUrl>https://raw.githubusercontent.com/danielpalme/ReportGenerator/master/LICENSE.txt</licenseUrl>
    <projectUrl>https://github.com/danielpalme/ReportGenerator</projectUrl>
    <projectSourceUrl>https://github.com/danielpalme/ReportGenerator</projectSourceUrl>
    <packageSourceUrl>https://github.com/danielpalme/ReportGenerator</packageSourceUrl>
    <docsUrl>https://github.com/danielpalme/ReportGenerator/wiki</docsUrl>
    <bugTrackerUrl>https://github.com/danielpalme/ReportGenerator/issues</bugTrackerUrl>
    <iconUrl>http://danielpalme.github.io/ReportGenerator/resources/logo_512.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>ReportGenerator converts XML reports generated by OpenCover, dotCover, Visual Studio, NCover, Cobertura, JaCoCo or Clover or into human readable reports in various formats. The reports do not only show the coverage quota, but also include the source code and visualize which line has been covered.</description>
    <summary>ReportGenerator converts XML reports generated by OpenCover, dotCover, Visual Studio, NCover, Cobertura, JaCoCo or Clover or into human readable reports in various formats. The reports do not only show the coverage quota, but also include the source code and visualize which line has been covered.</summary>
    <language>en-US</language>
    <tags>Code Coverage Reporting Testing TDD Utility</tags>
  </metadata>
  <files>
    <file src="..\..\target\bin\Release\LICENSE.txt" target="LICENSE.txt" />
    <file src="..\..\target\bin\Release\Readme.txt" target="Readme.txt" />

    <!-- NetCoreApp2.0 ('tools\netcoreapp2.0' directory) -->
    <file src="..\..\target\bin\Release\netcoreapp2.0\publish\**\*.*" target="tools\netcoreapp2.0" />
    
    <!-- .NET 4.7 ('tools\net47' directory) -->
    <file src="..\..\target\bin\Release\appsettings.json" target="tools\net47" />
    <file src="..\..\target\bin\Release\Microsoft.Build.*.dll" target="tools\net47" />
    <file src="..\..\target\bin\Release\Microsoft.Extensions.*.dll" target="tools\net47" />
    <file src="..\..\target\bin\Release\Microsoft.Win32.Primitives.dll" target="tools\net47" />
    <file src="..\..\target\bin\Release\Newtonsoft.Json.dll" target="tools\net47" />
    <file src="..\..\target\bin\Release\netstandard.dll" target="tools\net47" />
    <file src="..\..\target\bin\Release\ReportGenerator.Core.dll" target="tools\net47" />
    <file src="..\..\target\bin\Release\ReportGenerator.exe" target="tools\net47" />
    <file src="..\..\target\bin\Release\ReportGenerator.exe.config" target="tools\net47" />
    <file src="..\..\target\bin\Release\SixLabors.*.dll" target="tools\net47" />
    <file src="..\..\target\bin\Release\System.*.dll" target="tools\net47" />
  </files>
</package>
